
{
  "expo": {
    "name": "Fundee Cash",
    "slug": "fundee-cash",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/natively-dark.png",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/images/natively-dark.png",
      "resizeMode": "contain",
      "backgroundColor": "#bb86fc"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.fundeecash.app",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false,
        "UIAppFonts": [
          "SpaceMono-Regular.ttf",
          "SpaceMono-Bold.ttf",
          "SpaceMono-Italic.ttf",
          "SpaceMono-BoldItalic.ttf"
        ]
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/natively-dark.png",
        "backgroundColor": "#bb86fc"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.fundeecash.app"
    },
    "web": {
      "favicon": "./assets/images/final_quest_240x240.png",
      "bundler": "metro"
    },
    "plugins": [
      "expo-font",
      "expo-router",
      "expo-web-browser"
    ],
    "scheme": "fundee-cash",
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {}
    }
  }
}
